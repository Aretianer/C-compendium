//two files. 1st is main.cpp
#include <iostream>

int getInteger();

int main()
{
	int x = getInteger();
	int y = getInteger();

	std::cout << x << " + " << y << " is " << x + y << '\n';
	return 0;
}

//second is input.cpp
#include <iostream>

int getInteger()
{
	std::cout << "Enter an integer: ";
	int x;
	std::cin >> x;
	return x;
}
